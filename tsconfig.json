{
  "compilerOptions": {
    "module": "commonjs",
    "target": "es5",
    "strict": true,
    "allowJs": true,
    "checkJs": true,
    "noImplicitAny": true,
    "removeComments": true,
    "preserveConstEnums": true,
    // "outFile": "../../built/local/tsc.js",
    "sourceMap": true
  },
  "include": ["src/**/*"],
  "exclude": ["node_modules", "**/*.spec.ts"]
}